{
  "schema": "remote-schema.graphql",
  "documents": [
    "./src/**/*.tsx",
    "./src/**/*.ts"
  ],
  "extensions": {
    "codegen": {
      "overwrite": true,
      "generates": {
        "./src/types/__generated__/graphql.tsx": {
          "plugins": [
            "typescript",
            "typescript-operations",
            "typescript-react-apollo"
          ],
          "config": {
            "skipTypename": false,
            "withHooks": true,
            "withHOC": false,
            "withComponent": false
          }
        }
      }
    }
  }
}
